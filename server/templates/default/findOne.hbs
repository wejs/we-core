<table class="table table-striped table-hover">
  <tbody>
    {{#each record.dataValues }}
      <tr>
        <th>{{@key}}</th>
        <td>{{this}}</td>
      </tr>
    {{/each}}
  </tbody>
  <tfoot>
    <td colspan="2">
      {{we-action-edit-btn resourceName record req record.id}}
      {{we-action-delete-btn resourceName record req record.id}}
    </td>
  </tfoot>
</table>