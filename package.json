{
  "name": "we-core",
  "version": "2.2.2",
  "description": "We.js is a node.js framework for build real time applications, sites or blogs!",
  "homepage": "https://wejs.org",
  "main": "./src/index.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js -b ",
    "verbose-test": "NODE_ENV=test LOG_LV=verbose ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js -b ",
    "coverage": "NODE_ENV=test LOG_LV=info nyc mocha test/bootstrap.js test/**/*.test.js -b",
    "postversion": "git push"
  },
  "keywords": [
    "we.js",
    "wejs",
    "plugin",
    "wejs-plugin",
    "web",
    "api",
    "systems",
    "framework",
    "web framework"
  ],
  "repository": "wejs/we-core",
  "files": [
    "files",
    "src",
    "locales",
    "server",
    "install.js",
    "plugin.js"
  ],
  "author": "Alberto Souza <contato@albertosouza.net>",
  "license": "MIT",
  "dependencies": {
    "async": "^3.1.0",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "handlebars": "4.5.3",
    "lodash": "4.17.15",
    "mime": "2.4.4",
    "mkdirp": "0.5.1",
    "moment": "2.24.0",
    "morgan": "1.9.1",
    "pluralize": "^8.0.0",
    "request": "2.88.0",
    "sanitize-html": "1.20.1",
    "sequelize": "4.44.3",
    "serve-favicon": "~2.5.0",
    "simple-hooks-callback": "1.0.0",
    "string": "3.3.3",
    "uuid": "^3.3.3",
    "winston": "^2.4.3"
  },
  "devDependencies": {
    "chance": "^1.1.4",
    "connect-sqlite3": "^0.9.11",
    "express-mysql-session": "^2.1.0",
    "mocha": "6.2.2",
    "mysql2": "^2.1.0",
    "ncp": "^2.0.0",
    "nyc": "^15.0.0",
    "rimraf": "3.0.0",
    "sinon": "8.0.2",
    "sqlite3": "^4.1.1",
    "supertest": "4.0.2",
    "we-test-tools": "wejs/we-test-tools"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "wejs": {
    "plugins": {
      "we-plugin-post": true,
      "we-plugin-fastload": true
    }
  }
}
