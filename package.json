{
  "name": "we-core",
  "version": "2.1.1",
  "description": "We.js is a node.js framework for build real time applications, sites or blogs!",
  "homepage": "https://wejs.org",
  "main": "./src/index.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js -b ",
    "verbose-test": "NODE_ENV=test LOG_LV=verbose ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js -b ",
    "coverage": "NODE_ENV=test LOG_LV=info node_modules/.bin/istanbul cover -x src node_modules/.bin/_mocha -- -u exports -R spec test/bootstrap.js test/**/*.test.js",
    "postversion": "git push"
  },
  "keywords": [
    "we.js",
    "wejs",
    "plugin",
    "wejs-plugin",
    "web",
    "api",
    "systems",
    "framework",
    "web framework"
  ],
  "repository": "wejs/we-core",
  "files": [
    "files",
    "src",
    "locales",
    "server",
    "install.js",
    "plugin.js"
  ],
  "author": "Alberto Souza <contato@albertosouza.net>",
  "license": "MIT",
  "dependencies": {
    "async": "2.6.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-session": "1.16.1",
    "handlebars": "4.1.2",
    "lodash": "4.17.11",
    "mime": "2.4.2",
    "mkdirp": "0.5.1",
    "moment": "2.24.0",
    "morgan": "1.9.1",
    "pluralize": "^7.0.0",
    "request": "2.88.0",
    "sanitize-html": "1.20.1",
    "sequelize": "4.44.0",
    "serve-favicon": "~2.5.0",
    "simple-hooks-callback": "1.0.0",
    "string": "3.3.3",
    "uuid": "^3.3.2",
    "winston": "^2.4.3"
  },
  "devDependencies": {
    "chance": "^1.0.18",
    "express-mysql-session": "2.1.0",
    "istanbul": "0.4.5",
    "mocha": "6.1.4",
    "mysql2": "^1.6.5",
    "ncp": "^2.0.0",
    "pg": "^7.10.0",
    "pg-hstore": "^2.3.2",
    "rimraf": "2.6.3",
    "sinon": "7.3.2",
    "supertest": "4.0.2",
    "we-test-tools": "wejs/we-test-tools"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "wejs": {
    "plugins": {
      "we-plugin-post": true,
      "we-plugin-fastload": true
    }
  }
}
